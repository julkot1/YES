#import stdlib
_STATEMENT ADDS {
    IF {EQ ptc "sdsd"} 
       {ADD gr gr[ADD ptg {ADD 2 2}]}
       {IF {EQ ptc 2}
       {ADD cr cr[ADD ptc 1]}} 
};
ECHO {ADD xr[ADD ptx xr]};
IF {EQ xr gr} {
  PUSH (int) 1;
  PUSH IN;
  SUB gr gr[ADD ptg 1];
  ADD 3 {ADD 5 6}
  ECHO (Char)xr;
};
//gl -> global
//xr -> statements returns except PUSH, DROP, _STATEMENT
//cr -> context - in _STATEMENT parameters


